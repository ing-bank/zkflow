mod command_context;
use command_context::CommandContext;

fn verify(
    ctx: CommandContext,
) -> () {
    let signers = ctx.signers;
    dbg!("[Create] Signers {}", signers);

    let tx_mock_asset = ctx.outputs.mock_asset_contract_mock_asset_0;
    dbg!("[Create] TxState_MockAsset {}", tx_mock_asset);

    assert!(signers.contains(tx_mock_asset.data.owner.public_key), "[Create] Owner must sign");
    dbg!("[Create] Owner signed");

    dbg!("[Create] Contract rules verified");
}
