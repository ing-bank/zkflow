mod optional_u32;
use optional_u32::OptionalU32;

fn main(left: u32, right: u32, is_none: bool) -> u8 {
    let optional_left = OptionalU32 {
        is_none: is_none,
        inner: left
    };

    let optional_right = OptionalU32 {
        is_none: false,
        inner: right
    };

    if OptionalU32::equals(optional_left, optional_right) {
        0
    } else {
        1
    }
}